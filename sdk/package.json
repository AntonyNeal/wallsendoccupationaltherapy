{
  "name": "@your-organization/service-booking-sdk",
  "version": "1.0.0",
  "description": "JavaScript SDK for service booking platform with deployment templates",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "deployment-templates",
    "README.md",
    "GENERATORS-GUIDE.md",
    "INFRASTRUCTURE-GUIDE.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "node test-sdk.js",
    "prepublishOnly": "npm run build",
    "setup:vercel": "node scripts/install-deployment.js vercel",
    "setup:digitalocean": "node scripts/install-deployment.js digitalocean",
    "setup:netlify": "node scripts/install-deployment.js netlify"
  },
  "keywords": [
    "booking",
    "sdk",
    "api",
    "services",
    "scheduling",
    "typescript",
    "deployment",
    "vercel",
    "digitalocean",
    "netlify",
    "templates"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "undici": "^7.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/service-booking-platform-template.git",
    "directory": "sdk"
  },
  "homepage": "https://github.com/your-username/service-booking-platform-template#readme",
  "bugs": {
    "url": "https://github.com/your-username/service-booking-platform-template/issues"
  }
}
